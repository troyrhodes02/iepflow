{
  "name": "iepflow",
  "private": true,
  "version": "0.1.0",
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w @iepflow/web",
    "typecheck": "npm run typecheck -w @iepflow/web",
    "lint": "npm run lint -w @iepflow/web",
    "build": "turbo run build",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.5.8"
  }
}
